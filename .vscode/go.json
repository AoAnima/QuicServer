{

	"Обработчик err":{
		"prefix":"err",
		"body":[
			"if err != nil {",
			"	Ошибка(\" $1 %+v \\n\", err.Error())",
			"}"
		]
	},
    "Обработчик ошибок":{
		"prefix":"ош",
		"body":[
			"if ошибка != nil {",
			"	Ошибка(\" $1 %+v \\n\", ошибка)",
			"}"
		]
	},
	"Log":{
		"prefix":"log",
		"body":[			
			"Инфо(\"  %+v \\n\", ${1} )"
			
		]
	},
	"Инфо":{
		"prefix":"инфо",
		"body":[			
			"Инфо(\" %+v \\n\", ${1})"
			
		]
	},
	"Ошибка":{
		"prefix":"ошибка",
		"body":[			
			"Ошибка(\" %+v \\n\", ${1})"
			
		]
	}
}